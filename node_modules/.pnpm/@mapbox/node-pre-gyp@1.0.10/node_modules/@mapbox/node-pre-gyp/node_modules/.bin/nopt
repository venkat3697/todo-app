#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/myapp/server/node_modules/.pnpm/nopt@5.0.0/node_modules/nopt/bin/nopt.js/node_modules:/home/workspace/myapp/server/node_modules/.pnpm/nopt@5.0.0/node_modules/nopt/bin/node_modules:/home/workspace/myapp/server/node_modules/.pnpm/nopt@5.0.0/node_modules/nopt/node_modules:/home/workspace/myapp/server/node_modules/.pnpm/nopt@5.0.0/node_modules:/home/workspace/myapp/server/node_modules/.pnpm/node_modules:/home/workspace/myapp/server/node_modules:/home/workspace/myapp/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/myapp/server/node_modules/.pnpm/@mapbox/node-pre-gyp@1.0.10/node_modules/@mapbox/node-pre-gyp/node_modules:/home/workspace/myapp/server/node_modules/.pnpm/@mapbox/node-pre-gyp@1.0.10/node_modules/@mapbox/node_modules:/home/workspace/myapp/server/node_modules/.pnpm/@mapbox/node-pre-gyp@1.0.10/node_modules:/home/workspace/myapp/server/node_modules/.pnpm/@mapbox/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../nopt@5.0.0/node_modules/nopt/bin/nopt.js" "$@"
else 
  exec node  "$basedir/../../../../../../../nopt@5.0.0/node_modules/nopt/bin/nopt.js" "$@"
fi
